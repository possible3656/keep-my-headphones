<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.whybex.keepmyheadphones</string>

        <key>ProgramArguments</key>
        <array>
            <string>/usr/local/bin/KeepMyHeadphones</string>
        </array>

        <key>RunAtLoad</key>
        <true />

        <key>KeepAlive</key>
        <true />

        <key>StandardOutPath</key>
        <string>/tmp/keep-my-headphones.out.log</string>

        <key>StandardErrorPath</key>
        <string>/tmp/keep-my-headphones.err.log</string>

        <key>ProcessType</key>
        <string>Background</string>

        <key>Nice</key>
        <integer>1</integer>
    </dict>
</plist>